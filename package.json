{
  "name": "jizourappu",
  "version": "1.0.0",
  "private": true,
  "main": "node ./server/bin/www",
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "clean-webpack-plugin": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "babel": {
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "displayName": false,
          "minify": true
        }
      ],
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "start": "node ./server/bin/www",
    "server": "nodemon --signal SIGTERM ./server/bin/www",
    "dev": "NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js --progress --hide-modules",
    "build": "NODE_ENV=production webpack --config config/webpack.config.prod.js -p --progress --hide-modules --display-optimization-bailout"
  }
}
